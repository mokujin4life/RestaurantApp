<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byName">

       <bean id="employeeService" class="com.mokujin.service.EmployeeService"/>

       <bean id="employeeDAO" class="com.mokujin.dao.employee.IEmployeeDAO"/>

       <bean id="dishService" class="com.mokujin.service.DishService"/>

       <bean id="dishDAO" class="com.mokujin.dao.dish.IDishDAO"/>

       <bean id="ingredientService" class="com.mokujin.service.IngredientService"/>

       <bean id="ingredientDAO" class="com.mokujin.dao.ingredient.IIngredientDAO"/>
</beans>